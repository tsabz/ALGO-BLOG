<head>
    <%- include ('./partials/head.ejs') %>
</head>
  <div class="jumbo">
  <h1> ALGO BLOG</h1>
  </div>

<div class="editcontainer">
  <h1>Edit Yourz</h1>
    <form action="/algoblog/<%= allSongs.id%>?_method=PUT" method="POST">
      Title: <input type="text" name="title" placeholder="<%=allSongs.title%>"><br/>
      Song: <input type="text" name="song" placeholder="<%=allSongs.song%>"><br/>
      Entry: <input class="entry" type="textarea" name="entry" rows="4" cols= "50" placeholder="<%=allSongs.entry%>"><br/>
    <input type="submit" name="" value="Submit Changes"/>
  </form>
  <form action="/algoblog/<%=allSongs.id%>?_method=DELETE" method="POST">
    <input class="button" type="submit" value="Delete">
  </form>
    <a href="/algoblog/<%=allSongs.id%>">Back</a>
</div>
